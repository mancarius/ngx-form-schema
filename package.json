{
  "name": "ngx-form-schema-workspace",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 8400",
    "build": "ng build",
    "build:lib": "ng build ngx-form-schema",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false",
    "lint": "ng lint",
